{
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@now/node-server" }],
    "routes": [
        {
          "src": "/",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/artists/:first_letter",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/artist/:artist_name",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/lyrics/:artist/:title",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/search/:query",
          "dest": "/index.js",
          "methods": ["GET"]
        }
    ]
}